{
    "name": "samrat131/column-sortable",
    "description": "Sort laravel model view list by database column or field wise",
    "license": "MIT",
    "type": "package",
    "keywords": [
        "laravel-column-sortable",
        "laravel-sortable",
        "column-sortable",
        "column-sort",
        "sortable"
    ],
    "authors": [
        {
            "name": "Samrat",
            "email": "samrat0131@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Samrat131\\ColumnSortable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Samrat131\\ColumnSortable\\ColumnSortableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}